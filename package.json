{
  "name": "personal-website-v8",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "astro dev",
    "dev": "astro dev",
    "deploy:staging": "NODE_ENV=staging wrangler deploy --env staging",
    "deploy:prod": "NODE_ENV=production wrangler deploy --env production",
    "build:prod": "NODE_ENV=production astro build --mode production",
    "build:staging": "NODE_ENV=staging astro build --mode staging",
    "lint": "astro check",
    "lint:fix": "astro check --fix",
    "format": "prettier --no-error-on-unmatched-pattern --check */**/*.{js,json,ts,css,md,mdx,astro} --cache --cache-strategy metadata",
    "format:fix": "prettier --write */**/*.{js,json,ts,css,md,mdx,astro} --cache --cache-strategy metadata",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply blog-views-prod",
    "db:migrate:prod": "wrangler d1 migrations apply blog-views-prod --remote",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.3",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.2.7",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.4.0",
    "@playform/compress": "^0.1.9",
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "9.54.2",
    "@react-three/fiber": "8.10.1",
    "@react-three/postprocessing": "^3.0.4",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.6",
    "@types/gl": "^6.0.5",
    "add": "^2.0.6",
    "astro": "^5.7.13",
    "astro-capo": "^0.0.1",
    "astro-compressor": "^1.0.0",
    "astro-seo": "^0.8.4",
    "bun": "^1.2.13",
    "canvas": "^3.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.44.2",
    "gl": "^8.1.6",
    "lucide-react": "^0.511.0",
    "markdown-it": "^14.1.0",
    "react": "18",
    "react-dom": "18",
    "sanitize-html": "^2.17.0",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6",
    "three": "0.148.0",
    "three-stdlib": "2.21.6",
    "url-slug": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@types/bun": "^1.2.13",
    "@types/lodash.debounce": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/sanitize-html": "^2.16.0",
    "@types/three": "0.148.0",
    "drizzle-kit": "^0.31.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-visualizer": "^6.0.1",
    "sharp": "^0.34.2",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.22.0",
    "vite-plugin-glsl": "^1.4.2",
    "wasm-pack": "^0.13.1",
    "wrangler": "^4.14.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "bun",
    "canvas",
    "esbuild",
    "gl",
    "sharp",
    "wasm-pack",
    "workerd"
  ],
  "patchedDependencies": {
    "three-stdlib@2.21.6": "patches/three-stdlib@2.21.6.patch"
  }
}
